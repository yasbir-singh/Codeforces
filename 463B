#define int long long
#define nl "\n"
#define pi 2*acos(0.0)
#define setmin(x) memset((x),-1,sizeof((x)))
 //--------------------------------------------------------<solution>----------------------------------------------
void solve(){
    int n, t, m=0;
    cin >> n;
    for(int i=0; i<n; i++){
        cin >> t;
        if(m<t)m = t;
    }
    cout << m;
}
 //--------------------------------------------------------</solution>----------------------------------------------
signed main(){
    #ifdef LOCAL_MACHINE
        freopen("input.txt", "rt", stdin);
        freopen("output.txt", "wt", stdout);
    #endif
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    int t=1;
    //cin >> t;
    while(t--){
        solve();
    }
    return 0;
}
