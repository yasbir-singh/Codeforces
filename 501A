#define int long long
#define nl "\n"
#define pi 2*acos(0.0)
#define setmin(x) memset((x),-1,sizeof((x)))

 //--------------------------------------------------------<solution>----------------------------------------------
void solve(){
    int a, b, c, d;
    cin >> a >> b >> c >> d;
    a = max(3*a/10, a-(a/250)*c);
    b = max(3*b/10, b-(b/250)*d);
    if(a>b){
        cout << "Misha";
    } else if(b>a) {
        cout << "Vasya";
    } else {
        cout << "Tie";
    }
}
 //--------------------------------------------------------</solution>----------------------------------------------
signed main(){
    #ifdef LOCAL_MACHINE
        freopen("input.txt", "rt", stdin);
        freopen("output.txt", "wt", stdout);
    #endif
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    int t=1;
    //cin >> t;
    while(t--){
        solve();
    }
    return 0;
}
