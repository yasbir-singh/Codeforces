#include<bits/stdc++.h>
using namespace std;

int main(){
    int T;   // test case
    cin >> T;
    for(int tt=0; tt<T; tt++){
        
        int n;
        cin >> n;
        string s;
        cin >> s;
        
        // just check middle char it will be in every sub string.
        
        if(s[n-1]=='0'){
            for(int i=0; i<n; i++)cout << "0";
            cout << "\n";
        } else {
            for(int i=0; i<n; i++)cout << "1";
            cout << "\n";
        }
    }
}
